# Server Configuration
HOST=0.0.0.0
PORT=8080
# NODE_ENV=development

# Redis Configuration
REDIS_URL=redis://username:password@localhost:6379
REDIS_TTL=3600 # Cache TTL in seconds (default: 3600)
REDIS_TIMEOUT=5000 # Connection timeout in ms (default: 5000)

# Cache Configuration
CACHE_VERSION=v1 # Cache key version (default: v1)
CACHE_PRELOAD_ON_STARTUP=true # Preload cache on startup (default: true)
HTTP_FETCH_TIMEOUT=10000 # External API timeout in ms (default: 10000)
HTTP_FETCH_RETRIES=3 # Number of retry attempts (default: 3)

# Optional: Discord Badges (leave empty to disable)
DISCORD_TOKEN=

# Optional: GitHub Token (for better rate limits?)
# GITHUB_TOKEN=

BLOCKLIST_ENABLED=true # Enable IP/user blocklist (default: true)

# Optional: Admin API Key (required for cache management endpoints)
# ADMIN_API_KEY=your_secret_key_here
